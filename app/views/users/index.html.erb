<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
	  <th>Phone Number</th>
	  <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name + " " + user.last_name %></td>
        <td><%= user.email %></td>
		<td><%= user.phone_number %></td>
		<td><%= user.role %></td>
		
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
